    "import math \n",
    "\n",
    "def fit1MeV_Gaussian(v, par):\n",
    "    \n",
    "    arg = 0\n",
    "    if (par[2] != 0):\n",
    "        arg = (v[0]-par[1]/par[2])\n",
    "\n",
    "    fitval = par[0]*math.exp(-0.5*arg*arg)\n",
    "    fitval = (fitval * 2.00)/math.sqrt(math.tau*par[2])\n",
    "    \n",
    "    fitval = fitval + par[3] + v[0]*par[4]\n",
    "\n",
    "    return fitval"
   ]
